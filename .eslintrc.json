{
	"root": true,
	"extends": [
		"wikimedia/client",
		"wikimedia/jquery",
		"wikimedia/mediawiki"
	],
	"rules": {
		"eqeqeq": "off",
		"max-len": "off",
		"no-jquery/no-global-selector": "off",
		"no-jquery/no-sizzle": "off",
		"no-jquery/variable-pattern": "off",
		"no-redeclare": "off",
		"no-underscore-dangle": "off",
		"no-unused-vars": "off",
		"no-use-before-define": "off",
		"compat/compat": "warn",
		"no-jquery/no-extend": "warn",
		"es-x/no-object-assign": "warn",
		"no-var": "warn",
		"prefer-const": "warn"
	}
}
