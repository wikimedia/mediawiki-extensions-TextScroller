{
	"root": true,
	"extends": [
		"wikimedia/client",
		"wikimedia/jquery",
		"wikimedia/mediawiki"
	],
	"rules": {
		"eqeqeq": "off",
		"max-len": "off",
		"no-jquery/no-global-selector": "off",
		"no-jquery/no-sizzle": "off",
		"no-jquery/variable-pattern": "off",
		"no-redeclare": "off",
		"no-underscore-dangle": "off",
		"no-unused-vars": "off",
		"no-use-before-define": "off",
		"one-var": "off",
		"vars-on-top": "off"
	}
}
